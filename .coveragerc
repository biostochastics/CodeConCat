[run]
source = codeconcat
omit =
    */tests/*
    */venv/*
    */site-packages/*
    */env/*
    */dist/*
    */build/*
    
[report]
exclude_lines =
    pragma: no cover
    def __repr__
    raise NotImplementedError
    if __name__ == .__main__.:
    pass
    raise ImportError
    except ImportError
